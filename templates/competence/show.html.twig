{% extends 'base.html.twig' %}

{% block title %}Competence{% endblock %}

{% block body %}
    <h1>Competence</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ competence.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ competence.nom }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td>{{ competence.logo }}</td>
            </tr>
            <tr>
                <th>Other</th>
                <td>{{ competence.other ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('competence_index') }}">back to list</a>

    <a href="{{ path('competence_edit', {'id': competence.id}) }}">edit</a>

    {{ include('competence/_delete_form.html.twig') }}
{% endblock %}
